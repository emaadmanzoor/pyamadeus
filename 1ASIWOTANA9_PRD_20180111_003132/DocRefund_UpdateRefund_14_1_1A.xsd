<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/TRFUUQ_14_1_1A" xmlns="http://xml.amadeus.com/TRFUUQ_14_1_1A" elementFormDefault="qualified">
  <xs:element name="DocRefund_UpdateRefund">
    <xs:annotation>
      <xs:documentation xml:lang="en">Refund Update query
Refund update message
Clone of TFRUUQ 12.2.1  Update done for tracking purpose. Update of Data Element properties of composite 5004  and 5482 of CMN segment. XML type updated from Integer to Decimal.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="userIdentification" type="UserIdentificationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">User identification mainly used for the agent code and the agent sign.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ticketNumber" minOccurs="0" type="TicketNumberTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">the ticket number of the refundable documents, inclusive of airline and check digit.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="dateTimeInformation" maxOccurs="2" type="StructuredDateTimeInformationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Current date (710)  date of refund (DR) (if any) of the document.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="referenceInformation" minOccurs="0" type="ReferenceInfoType">
          <xs:annotation>
            <xs:documentation xml:lang="en">References</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="travellerInformation" minOccurs="0" type="TravellerInformationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Information on the passenger to refund</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ticket" minOccurs="0" maxOccurs="7">
          <xs:annotation>
            <xs:documentation xml:lang="en">Ticket details</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ticketInformation" type="TicketNumberTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Ticket to refund information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ticketGroup" minOccurs="0" maxOccurs="4">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Group for ticket informations</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="couponInformationDetails" type="CouponInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Coupons information details. Each ticket must contain 4 coupons information.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="boardingPriority" minOccurs="0" type="PriorityTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Use to update boarding priority for Staff.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="actionIdentification" minOccurs="0" type="ActionIdentificationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Used for Staff update flag.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="referenceInformation" minOccurs="0" type="ReferenceInfoType_57269S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Used for Staff Validation Certificate</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="travellerPriorityInfo" minOccurs="0" type="TravellerPriorityDetailsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Use for Staff: Staff number, Date of Joining and Cost Center.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="monetaryInformation" minOccurs="0" type="MonetaryInformationType" />
        <xs:element name="pricingDetails" minOccurs="0" type="PricingTicketingDetailsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Pricing details</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="commission" minOccurs="0" type="CommissionInformationType" />
        <xs:element name="taxDetailsInformation" minOccurs="0" maxOccurs="198" type="TaxTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">List of refund taxes (up to 197 taxes may be present+one aggregate sum of previously paid taxes).  Previously paid taxes aggregate amount should use the tax category 701, and the tax type XT.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="tourInformation" minOccurs="0" type="TourInformationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Tour information</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="interactiveFreeText" minOccurs="0" maxOccurs="3" type="InteractiveFreeTextType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Conveys different information carried as free-text: - Remarks - ...</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="fopGroup" minOccurs="0" maxOccurs="99" type="FOPRepresentationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Fop group (up to 99 fops)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="transactionContext" minOccurs="0" type="TransactionContextType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Transaction context used for credit card concealment purposes.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="firstAndLastSegmentDates" minOccurs="0" type="StructuredPeriodInformationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Conveys the date of the first and the last segment of the original coupons</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="originAndDestination" minOccurs="0" type="OriginAndDestinationDetailsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">The routing field identifies what routing the document was to be refunded. For manual refund routing information is not available and this will be up to the agent to enter routing in order to be able to calculate VAT. In cases where a manual document needs to be refunded the routing indicator is mandatory. (For full and partial refund this element can not be entered)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="refundedItinerary" minOccurs="0" maxOccurs="16" type="RefundedItineraryType">
          <xs:annotation>
            <xs:documentation xml:lang="en">This default group is used to convey the refunded itinerary. The refunded itinerary is composed of the origin city/airport code, the flight operating airline code and the destination city/airport code. Origin and destination city/airport codes are contained in the ODI segment. Operating airline code is conveyed in the TRA segment. This default group can be repeated up to 16 times since a document can be composed of up to 16 coupons.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="refundedRoute" minOccurs="0" type="RoutingInformationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">The routing coresponding to the refunded itinerary.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="structuredAddress" minOccurs="0" type="StructuredAddressType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Used to convey any kind of structured address. For the time being, only billing address (AB) is supported.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="feeGroup" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Fees group</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="feeType" type="SelectionDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Fee type</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="feeSubGroup" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">fee Sub Group</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="feeSubType" type="SpecificDataInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">fee sub type</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="feeMonetaryInfo" type="MonetaryInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">fee monetary Info</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="feesTaxes" minOccurs="0" maxOccurs="99" type="TaxTypeI_55307S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Fee taxes</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ActionIdentificationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide details of action required or taken, the flights to which this action refers, and reasons for action taken.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="actionRequestCode" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">provides type of required action, coded.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommissionDetailsType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the commission conditions, amount, currency, party paying and any related free text.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission type : 'NEW' --) New commission 'OLD' --) Old Commission 'XLP' --) Commission on cancellation Penalty 'FMA' --) Airline Commission A 'FMB' --) Airline Commission B</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amount" minOccurs="0" type="NumericDecimal_Length1To18">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="freeText" minOccurs="0" type="AlphaNumericString_Length1To70" />
      <xs:element name="rate" minOccurs="0" type="NumericDecimal_Length1To8">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission percentage</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommissionInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify information about a commission.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="commissionDetails" type="CommissionDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission details</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherComDetails" minOccurs="0" maxOccurs="8" type="CommissionDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Code or name to identify a company and any associated companies.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="operatingCompany" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Used to convey an airline code. The airline code can be replaced by -- (in case of ARNK segment).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CouponInformationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the coupon number, status, value, and other related information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cpnNumber" type="AlphaNumericString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Coupon number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cpnStatus" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Coupon status</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="settlementAuthorization" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Settlement Authorisation Code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CouponInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify data specific to a value (flight) coupon.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="couponDetails" type="CouponInformationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Coupon information details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DataInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify specific data and a quantity related to the data.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="indicator" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">The carrier fee application code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DataTypeInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of data to be sent and to qualify the data when required.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">To specify the type of data, i.e. staff reservation entitlement, status, type of travel, or ticket type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="statusEvent" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Reservation entitlement/status, type of travel, ticket type</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FOPRepresentationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">This group will be used to convey all fop informations</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="formOfPaymentInformation" type="FormOfPaymentTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Form of payment</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="interactiveFreeText" maxOccurs="2" type="InteractiveFreeTextType">
        <xs:annotation>
          <xs:documentation xml:lang="en">This interactive free text is used to pass the complete form of payment and the  customer file reference fields</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FormOfPaymentDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the form of payment type, amounts, approval codes, and other related information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">the type of the fop</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amount" minOccurs="0" type="NumericDecimal_Length1To18">
        <xs:annotation>
          <xs:documentation xml:lang="en">Form of payment amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sourceOfApproval" minOccurs="0" type="AlphaNumericString_Length1To3" />
      <xs:element name="authorisedAmount" minOccurs="0" type="NumericInteger_Length1To18" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FormOfPaymentTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey details describing the form of payment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="formOfPayment" type="FormOfPaymentDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Form of payment details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FreeTextQualificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the type, purpose, and language of free text and whether any action is required.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="textSubjectQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Text subject qualifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="informationType" minOccurs="0" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Text type information to identify the IFT segment data.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InteractiveFreeTextType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide free form or coded text information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="freeTextQualification" minOccurs="0" type="FreeTextQualificationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Free text qualification</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="freeText" type="AlphaNumericString_Length1To126">
        <xs:annotation>
          <xs:documentation xml:lang="en">Free text</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MonetaryInformationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the type of monetary amount, the amount, and the currency code.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="typeQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">A code describing an amount.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amount" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Amount, 'FREE', 'BULK'</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="currency" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">ISO currency code.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="locationFrom" minOccurs="0" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">The From city for the carrier fee</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="locationTo" minOccurs="0" type="AlphaNumericString_Length1To25" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MonetaryInformationDetailsType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the type of monetary amount, the amount, and the currency code.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="typeQualifier" type="AlphaNumericString_Length1To3" />
      <xs:element name="amount" minOccurs="0" type="NumericDecimal_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="currency" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Currency</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MonetaryInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey monetary amounts, rates and percentages.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="monetaryDetails" maxOccurs="20" type="MonetaryInformationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Fee amount</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MonetaryInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey monetary amounts, rates and percentages.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="monetaryDetails" type="MonetaryInformationDetailsType" />
      <xs:element name="otherMonetaryDetails" minOccurs="0" maxOccurs="98" type="MonetaryInformationDetailsType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OriginAndDestinationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey the origin and destination of a journey.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="origin" minOccurs="0" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">City code or country code of the start of the original document. Example: DE or MUC</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="destination" minOccurs="0" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">City code or country code of the end of the original document. Example: DE or MUC If the start of the original document is a city code, then the end must be specified as a city code also. If the start of the original document is a country code, then the end must be specified as a country code also.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OriginAndDestinationDetailsTypeI_67857S">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey the origin and destination of a journey.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="origin" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element will convey the origin city/airport code corresponding to the coupon being refunded.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="destination" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element will convey the destination city/airport code corresponding to the coupon being refunded.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OriginatorIdentificationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the originator of the travel request.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="originatorId" type="NumericInteger_Length1To8">
        <xs:annotation>
          <xs:documentation xml:lang="en">Agent code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PricingTicketingDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey pricing and date information related to a ticket.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="priceTicketDetails" minOccurs="0" type="PricingTicketingInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Pricing details indicators</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="priceTariffType" minOccurs="0" type="AlphaNumericString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indication of NR when the ticket to refund has been issued in Net Remit negociated fare.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PricingTicketingInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify indicators related to pricing and ticketing.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="indicators" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicators for : - Domestic/Internationnal itinerary statistical code This information may be not present in case of MCO refund (Miscellaneous Charges Order) as the notion of itinerary is irrelevant in that case.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PriorityDetailsTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To indicate a priority.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
