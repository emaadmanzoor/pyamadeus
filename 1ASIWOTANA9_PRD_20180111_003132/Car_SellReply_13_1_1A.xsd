<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/RCSSCR_13_1_1A" xmlns="http://xml.amadeus.com/RCSSCR_13_1_1A" elementFormDefault="qualified">
  <xs:element name="Car_SellReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Car sell response to API-V2/EDI powered client
This message is the Car sell response
The message is containing all the information regarding the car sell created in the PNR.  R for CAR CSS for Car Sell C for Create R for Reply  Maximum size : 3 Kbytes Average size : 1 Kbytes Sender : Car TPF Receiver : received by the API-V2/EDI powered client</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="errorWarning" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">This group is used to convey the errors</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="applicationError" type="ApplicationErrorInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Error/warning</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="errorFreeText" minOccurs="0" type="InteractiveFreeTextTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Error or warning freetext</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="pnrRecLoc" minOccurs="0" type="ReservationControlInformationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">PNR record Locator</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="carSegment" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">This group contains all the CAR sell data</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="bookingIdentifier" type="ItemReferencesAndVersionsType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This segment is used to convey the segment tattoo of the car sell.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="rateStatus" type="RelatedProductInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Car segment status. The possible values corresponding to the PNR display are the following: - HK (C/A car segment) - SS (Std/A car segment available) - NN (Std/A car segment on request)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="bookingType" minOccurs="0" type="SelectionDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Contain the Access level or booking type requested by the client.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="marketingInfo" minOccurs="0" maxOccurs="2" type="InteractiveFreeTextTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Contains: 1) up to 6 lines of marketing text sent by the car provider. 2) up to 3 lines of Other services messages (advertisments).</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="paxName" minOccurs="0" type="SpecificTravellerTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Passenger name that rents the car</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="arrivalInfo" minOccurs="0" type="FlightDetailsQueryType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Flight information associated to the arrival time to the CAR agency.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="typicalCarData">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Car Sell details.  This group contains the data not already included in the PNRACC</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="companyIdentification" type="CompanyInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Car provider code</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="locationInfo" maxOccurs="6" type="PlaceLocationIdentificationTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This segment is used to convey the following information:  1 - Pickup Location : (Iata code or 1A code or Car Provider location reference)  2 - Pickup Location : Correcponding Car Provider location reference if the pickup location, specified by the API user, is a 1A one.  3 - DropOff location : Iata code or 1A code or Car provider Location reference  4 - Collection location /COL   5 - delivery location /DEL</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="deliveryAndCollection" minOccurs="0" maxOccurs="2" type="DeliveryAndCollectionType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This group is used to carry Delivery and Collection information in a structured way.  This group is exclusive with LOC segment which have 2 repetitions to carry unstructured Delivery and Collection information.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="pickupDropoffTimes" type="StructuredPeriodInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Pickup and dropoff dates and times</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="vehicleInformation" type="VehicleInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Vehicle information - vehicle type (SIPP code), - vehicle special equipments</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="confirmationNbr" minOccurs="0" type="ReservationControlInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Confirmation number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="rateCodeInfo" minOccurs="0" type="FareQualifierDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Rate code</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="fFlyerNbr" minOccurs="0" type="FrequentTravellerIdentificationCodeType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Frequent flyer number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="customerInfo" minOccurs="0" type="ConsumerReferenceInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Customer Information: Customer preferences associated to the Car provider. Can be a Customer Idendification  Number (ID) and/or a Corporate Discount number (CD) and/or a Promotional Code (PC).</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="rateInfo" minOccurs="0" maxOccurs="8" type="TariffInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This segments is used to convey: 1)General Rate information (identifier, plan, category) and Unstructured RB/RQ/RG 2) Structured rate quoted (RQ) or guaranted (RG) 3) Structured base rate (RB) 4) Estimated total information 5) Drop amount data 6) Voucher coupon print references (VV) 7) Mark-up amount 8) Selling price</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="payment" minOccurs="0" type="FormOfPaymentTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">- Form of payment (FP) - Form of guarantee (G)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="attributeData" minOccurs="0" type="CreditCardAttributeDataType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Conveys the CC attribute data for the authorisation request.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="billingData" minOccurs="0" type="BillableInformationTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">- Billing reference number (contains conpany data to be built) - Billing mumber (number included in the billing) - Agency account</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="bookingSource" minOccurs="0" type="AdditionalBusinessSourceInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Iata agency code</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="inclusiveTour" minOccurs="0" type="TourInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Tour code</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="supleInfo" minOccurs="0" type="MiscellaneousRemarksType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This segment is used to convey the supplementary  informations (SI). e.g: "Customer arriving after agency closure hour. Car keys waiting at the hotel reception located next to the agency"</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="estimatedDistance" minOccurs="0" maxOccurs="2" type="QuantityTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This segment is used to convey distances.  1) Intercity distance. Distance between the Pickup and the Dropoff cities. Information returned by the Car provider for customer notification purpose.  2) Estimated distance Distance that is going to be runned during the rental period.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="agentInformation" minOccurs="0" type="NameTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Booking agent name</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="trackingOpt" minOccurs="0" type="AgreementIdentificationTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Tracking Option (TK)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="customerEmail" minOccurs="0" type="CommunicationContactTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Customer e-mail</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="attribute" minOccurs="0" type="AttributeType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">It specifies if the booking is leisure.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="paxTattooNbr" type="ReferenceInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Passenger tattoo number</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AdditionalBusinessSourceInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify ownership and creator of a PNR</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="originatorDetails" type="OriginatorIdentificationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">ORIGINATOR DETAILS</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressDetailsTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the details of an address.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="format" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address format</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="line1" type="AlphaNumericString_Length1To60">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address Field in free flow text</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify an address</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="addressUsageDetails" type="AddressUsageTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address Type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="addressDetails" minOccurs="0" type="AddressDetailsTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Format 1 - Home Delivery/Collection</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="city" minOccurs="0" type="AlphaNumericString_Length1To30">
        <xs:annotation>
          <xs:documentation xml:lang="en">City name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zipCode" minOccurs="0" type="AlphaNumericString_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">Postal Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="countryCode" minOccurs="0" type="AlphaNumericString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Country code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="regionDetails" minOccurs="0" type="CountrySubEntityDetailsTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey a sub-entity within a country : region, states..</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="locationDetails" minOccurs="0" type="LocationIdentificationTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Format 2 - Site Delivery/Collection</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressUsageTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To describe the usage of an address</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="purpose" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address Type: - DEL for Delivery - COL for Collection</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AgreementIdentificationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the agreement details.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="agreementDetails" minOccurs="0" type="AgreementTypeIdentificationTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Agreement identification</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AgreementTypeIdentificationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">Identification of specific agreement type by code or name.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="code" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">- TK for Tracking option</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">Agreement description</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorDetailType">
    <xs:sequence>
      <xs:element name="errorCode" type="AlphaNumericString_Length1To5">
        <xs:annotation>
          <xs:documentation xml:lang="en">Code identifying the data validation error condition.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="errorCategory" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Identification of a code list.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="errorCodeOwner" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Code identifying the agency responsible for a code list.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of application error within a message.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="errorDetails" type="ApplicationErrorDetailType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Application error details.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AssociatedChargesInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Identification of the additional charges that are associated to a product or service.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Defines the type of the charge : extra day/hour charges, free mileage, mileage charge, policy charges. See code set values</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amount" minOccurs="0" type="NumericDecimal_Length1To12">
        <xs:annotation>
          <xs:documentation xml:lang="en">Mileage charge amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" minOccurs="0" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">To qualify the amount, can be - UNL (for unlimited mileage) when used for free mileage - 3 (for included in base rate) - 4 (for not included in base rate)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberInParty" minOccurs="0" type="NumericInteger_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Quantity of free mileage</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="comment" minOccurs="0" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">Unstructured RG,RG and RQ rates.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributeInformationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of attribute and the attribute</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="attributeType" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">the attribute type LEI for leisure booking</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="attributeDescription" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Not Used</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used to have tag value without code list for tag</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="criteriaSetType" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Specify which attribute is described in E003. BAT for booking attribute</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="criteriaDetails" type="AttributeInformationTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Details for the attribute type.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BillableInformationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide billable information for a claim or encounter.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="billingInfo" maxOccurs="3" type="DiagnosisTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">billing number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommunicationContactDetailsTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">Communication number of a department or employee in a specified channel.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="email" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">Email</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="contactQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Contact qualifier. EM for Electronic mail</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommunicationContactTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">email</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="contact" type="CommunicationContactDetailsTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Customer contact</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">This segment is used to convey the non-air company information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="travelSector" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to qualify the company code, to identify the industry business it belongs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyCode" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the company code of a non-air company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyName" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the company name of a non-air company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accessLevel" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the type of access the non-air company has with Amadeus.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConsumerReferenceIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Identification of consumer references by type, company, number and reference to a traveller.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="referenceQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicator - 1  for ID (customer number) - CD for CD (customer discount number) - PC for PC (promotional code)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="referenceNumber" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Either the ID number, the CD number or the PC number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConsumerReferenceInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify consumer reference information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="customerReferences" type="ConsumerReferenceIdentificationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Consumer reference information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherCustomerRef" minOccurs="0" maxOccurs="2" type="ConsumerReferenceIdentificationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Additional consumer reference information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CountrySubEntityDetailsTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify a part of a country (eg county or part of              a city).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="code" type="AlphaNumericString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">State Code. Mandatory if CountryCode is US, CA, AU.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CreditCardAttributeDataType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Conveys attribute data of the credit card</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="securityId" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Conveys the security ID of the Credit Card.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DeliveryAndCollectionType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Delivery and Collection address</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="addressDeliveryCollection" type="AddressTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">This Segment is used to Delivery and Collection information:  Format 1- (Home Collection): - Address - City - State - Country - Zip Code  Format 2- (Site Collection): - Site Ref Id - Site Name</xs:documentation>
