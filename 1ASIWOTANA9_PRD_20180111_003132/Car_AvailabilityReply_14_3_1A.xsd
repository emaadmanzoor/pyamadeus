      <xs:element name="chargeType" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Type of the charge :  108 Surcharge 13 Special equipment 45 Tax COL Collection COV Coverage CPN Coupon DEL Delivery</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amount" minOccurs="0" type="NumericDecimal_Length1To12">
        <xs:annotation>
          <xs:documentation xml:lang="en">Charge  amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amountQualifier" minOccurs="0" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">The possible values of this qualifier are: - 3    included - 4    not included  - UNL  Unlimited mileage (used for free mileage) - IES included in Estimated Total - IBR included in Base Rate - OPT Optional - MAN Mandatory - NBR Not Included in Base Rate - ITX Policy amount Includes Tax - NTX Policy amount Not Includes Tax - CNF Confirmed - NAV Not Available - ONR On Request</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="quantity" minOccurs="0" type="NumericInteger_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Maximum days</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="chargePeriodTypeCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Charge period type coded :  001 per day 002 per week 003 per month 004 per rental 012 tax percentage 013 no value available FDP from day period FMP from month period FWP from week period TDP to day period TMP to month period TWP to week period PMD per modification</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="currencyCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Currency code of the amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="chargeCode" minOccurs="0" type="AlphaNumericString_Length1To5">
