              <xs:documentation xml:lang="en">Fare adjustments based on accommodation type.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:complexContent>
                <xs:extension base="CodeAmountType" />
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
          <xs:element name="FareQualifierAdjustment" type="CodeAmountType" minOccurs="0" maxOccurs="99">
            <xs:annotation>
              <xs:documentation xml:lang="en">Other fare qualifier adjustments.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DiscountClass" minOccurs="0" maxOccurs="99">
            <xs:annotation>
              <xs:documentation xml:lang="en">Fare promotions and other qualified discounts.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attributeGroup ref="DiscountInfoGroup" />
              <xs:attributeGroup ref="TravelerCountGroup" />
            </xs:complexType>
          </xs:element>
          <xs:element name="AncillaryCharge" type="CodeAmountType" minOccurs="0" maxOccurs="99">
            <xs:annotation>
              <xs:documentation xml:lang="en">Ancillary product and service charges.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Fee" type="CodeAmountType" minOccurs="0" maxOccurs="99">
            <xs:annotation>
              <xs:documentation xml:lang="en">Applicable Fee code and associated amount.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Tax" type="CodeAmountType" minOccurs="0" maxOccurs="9">
            <xs:annotation>
              <xs:documentation xml:lang="en">Applicable tax code and associated amount.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="RailPriceType">
