<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/TARFRQ_16_1_1A" xmlns="http://xml.amadeus.com/TARFRQ_16_1_1A" elementFormDefault="qualified">
  <xs:element name="DocRefund_CalculateRefund">
    <xs:annotation>
      <xs:documentation xml:lang="en">This message enables airlines, airline sevice suppliers and their trading partners to request the automatic calculation of a ticket refund.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="messageAction" type="MessageActionDetailsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">This field is used to specify if the request is done with ticket data (no call to ticket server to retrieve the E-ticket) or without ticket data (call to ticket server to retrieve e-ticket).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="passengerInfoGroup" minOccurs="0" maxOccurs="9">
          <xs:annotation>
            <xs:documentation xml:lang="en">Passenger Information</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="segmentRepetitionControl" type="SegmentRepetitionControlType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Contains: * Number of passengers in the group * Group tattoo</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="travellersID" minOccurs="0" maxOccurs="9" type="SpecificTravellerType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Traveller identification: tatoo, names, birth date.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="discountPtc" type="FareInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Passenger PTC</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="refundInformationGroup" maxOccurs="9">
          <xs:annotation>
            <xs:documentation xml:lang="en">Tickets to be refunded group</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="transactionIdentifier" type="ItemNumberType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This segment conveys the identifier of the transaction.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ticketInfoPerPaxGroup" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Group of ticket information. One group per passenger (one ticket per passenger).</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="passengerReference" type="SegmentRepetitionControlTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Passenger Reference Number the ticket data are associated to (referenceNumber in Gr3.STD)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ticketingAgencyId" minOccurs="0" type="UserIdentificationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Identification of the ticketing agency</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="additionalTicketingAgencyInfo" minOccurs="0" type="AdditionalBusinessSourceInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Additional Ticketing agency information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ptcInfo" minOccurs="0" type="FareInformationTypeI_82318S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Passenger Type Code the ticket has been priced with.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="actionDetails" minOccurs="0" type="StatusTypeI" />
                    <xs:element name="rapidrewardaccount" minOccurs="0" type="FrequentTravellerIdentificationCodeType" />
                    <xs:element name="monetaryInfo" minOccurs="0" type="MonetaryInformationType" />
                    <xs:element name="taxInfo" minOccurs="0" maxOccurs="5" type="TaxTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Tax information of the ticket</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="interactiveFreeText" minOccurs="0" maxOccurs="9" type="InteractiveFreeTextTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Ticket free text information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="formOfPayment" minOccurs="0" type="FormOfPaymentTypeI_67087S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Ticket Form of Payment</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="tourCodeInfo" minOccurs="0" type="TourInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Ticket tour code</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ticketNumberGroup" maxOccurs="4">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Ticket information.  One group per ticket. Up to 4 conjonctive tickets for a passenger.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ticketNumberInfo" type="TicketNumberTypeI_66556S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Ticket number.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="issueIdentifier" minOccurs="0" type="SpecificDataInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Issue Identifier - First Issue or Reissue</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ticketingDetails" minOccurs="0" type="PricingTicketingDetailsTypeI_67043S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Ticketing information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="couponInfoGroup" minOccurs="0" maxOccurs="4">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">If MSG = 135 (refund request with ticket data) : Group of coupon details. Up to 4 coupons per ticket. If MSG = 136 (refund request with ticket numbner only) : Coupon selection in case of paper ticket refund.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="couponInfo" type="CouponInformationTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Coupon information - number and status</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="segmentInfo" minOccurs="0" type="TravelProductInformationTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Segment information - origin, destination, flight number, ...</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="couponPricingSubsequent" minOccurs="0" type="PricingTicketingSubsequentTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Coupon pricing information - fare basis and additional info</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="couponDateTimeInfo" minOccurs="0" type="DateAndTimeInformationTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">NVA and NVB dates</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="ticketingDetails" minOccurs="0" type="PricingTicketingDetailsTypeI" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="firstPricingInformationKey" minOccurs="0" type="BinaryDataType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Reference code to retreive first pricing transaction</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="feeGroup" minOccurs="0" maxOccurs="9">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Ob fee group</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="feeTypeInfo" type="SelectionDetailsTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fee Type</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="feeSubGroup" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fee Breakdown group</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="feeSubType" type="SpecificDataInformationTypeI_67068S" />
                                <xs:element name="feeMonetaryInfo" type="MonetaryInformationTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Fee monetary info</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="feeFormOfPayment" minOccurs="0" type="FormOfPaymentTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Fee Form of Payment</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="feeTaxes" minOccurs="0" type="TaxTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Taxes applicable on a fee</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="pricingOptionGroup" minOccurs="0" maxOccurs="99">
          <xs:annotation>
            <xs:documentation xml:lang="en">It allows to enter, for each option, the name of the option, the applicable attributes (in dedicated segments) and association. The group is to be repeated once per applicable pricing option Pricing options group - city override, currency override, corporate fare</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="pricingOptionKey" type="PricingOptionKeyType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">the smart name of the option</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="optionDetail" minOccurs="0" type="AttributeType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option value takes a free text</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="carrierInformation" minOccurs="0" type="TransportIdentifierType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option value takes a carrier code</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="currency" minOccurs="0" type="CurrenciesType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option value takes a currency code</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="penDisInformation" minOccurs="0" type="DiscountAndPenaltyInformationType" />
              <xs:element name="monetaryInformation" minOccurs="0" type="MonetaryInformationType" />
              <xs:element name="taxInformation" minOccurs="0" maxOccurs="99" type="DutyTaxFeeDetailsType" />
              <xs:element name="dateInformation" minOccurs="0" maxOccurs="2" type="StructuredDateTimeInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option value takes a date</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="frequentFlyerInformation" minOccurs="0" type="FrequentTravellerIdentificationCodeType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option deals with Frequent Flyer Information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="formOfPaymentInformation" minOccurs="0" type="FormOfPaymentType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option deals with Form Of Payment</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="locationInformation" minOccurs="0" type="PlaceLocationIdentificationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option value takes location code.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ticketInformation" minOccurs="0" type="TicketNumberTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">in case the option value takes a ticket number.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="couponInformation" minOccurs="0" type="CouponInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To identify the coupon to which the option will be aplicable.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="paxSegTstReference" minOccurs="0" type="ReferenceInfoType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">to associate the option to a passenger or a segment.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AdditionalBusinessSourceInformationType">
    <xs:annotation>
