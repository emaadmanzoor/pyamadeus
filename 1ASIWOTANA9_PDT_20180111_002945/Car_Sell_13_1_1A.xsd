<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/RCSSCQ_13_1_1A" xmlns="http://xml.amadeus.com/RCSSCQ_13_1_1A" elementFormDefault="qualified">
  <xs:element name="Car_Sell">
    <xs:annotation>
      <xs:documentation xml:lang="en">This message is a CAR sell request</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="pnrInfo">
          <xs:annotation>
            <xs:documentation xml:lang="en">PNR record locator and PAX tattoo number</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="paxTattooNbr" type="ReferenceInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Passenger tattoo number</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pnrRecLoc" minOccurs="0" type="ReservationControlInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">PNR record Locator</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="sellData">
          <xs:annotation>
            <xs:documentation xml:lang="en">Data to build the sell</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="companyIdentification" type="CompanyInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Company code, and compnay access level requested.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="sellFromAvailabilitylGroup" minOccurs="0">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This group is used to convey the information necessary to perform a sell from complete access availability : - validity period of each rate (in minutes) - encrypted data 1 - encrypted data 2 The encrypted data must be returned to the CAR application when doing a sell from complete access availability so that the CAR application can decide if the sell is authorized or not.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="validityPeriod" type="QuantityType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">In this segment we have the validity period of the considered rate (i.e. a sell from complete access availability can be done during the validity period)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="validityTokens" type="ReferenceInfoType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This segment contains the tokens that will be returned on a car sell request to be able to perform a complete access plus sell (sell from CA+ availability).</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="bookingType" minOccurs="0" type="SelectionDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Used to request a particular Booking access type.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="locationInfo" maxOccurs="4" type="PlaceLocationIdentificationTypeU">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This segment is used to convey the following information: - Pickup Location, - DropOff location, - Collection location /COL  - delivery location /DEL</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="deliveryAndCollection" minOccurs="0" maxOccurs="2" type="DeliveryAndCollectionType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This group is used to carry Delivery and Collection information in a structured way.  This group is exclusive with LOC segment which have 2 repetitions to carry unstructured Delivery and Collection information.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pickupDropoffTimes" type="StructuredPeriodInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Pickup and dropoff dates and times</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="vehicleInformation" type="VehicleSelectionType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Vehicle selection criteria - Vehicle type - Special vehicle equipments (up to 5)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="arrivalInfo" minOccurs="0" type="FlightDetailsQueryType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Flight information associated to the arrival time to the CAR agency.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="rateCodeInfo" minOccurs="0" type="FareQualifierDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To specify a rate code: Standard, Inclusive, Industry, Corporate, Credential, Government, Association, Travel Inductry, Consortium,  Convention, Package</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="fFlyerNbr" minOccurs="0" type="FrequentTravellerIdentificationCodeType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Frequent flyer number</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="customerInfo" minOccurs="0" type="ConsumerReferenceInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Customer Information: Customer preferences associated to the Car provider. Can be a Customer Idendification  Number (ID) and/or a Corporate Discount number (CD) and/or a Promotional Code (PC).</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="rateInfo" minOccurs="0" maxOccurs="2" type="TariffInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Segment used to vehiculate the follwing  sets of data :  1)Rate details (Rate Category and Rate Plan)   2) Rate Quoted to the customer (Currency and amount)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="payment" minOccurs="0" type="FormOfPaymentTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">- Form of payment (FP) - Form of guarantee (G)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="attributeData" minOccurs="0" type="CreditCardAttributeDataType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the CC attribute data for the authorisation request.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="billingData" minOccurs="0" type="BillableInformationTypeU">
                <xs:annotation>
                  <xs:documentation xml:lang="en">- Billing reference number (contains conpany data to be build) - Billing mumber (number included in the billing) - Agency account</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="bookingSource" minOccurs="0" type="AdditionalBusinessSourceInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Iata agency code</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="inclusiveTour" minOccurs="0" type="TourInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Tour code</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="supleInfo" minOccurs="0" type="MiscellaneousRemarksType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This segment is used to convey the supplementary  informations (SI). e.g: "Customer arriving after agency closure hour. Car keys waiting at the hotel reception located next to the agency"</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="estimatedDistance" minOccurs="0" type="QuantityTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Estimated distance</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="agentInformation" minOccurs="0" type="NameTypeU">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Booking agent name</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="trackingOpt" minOccurs="0" type="AgreementIdentificationTypeU">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Tracking Option (TK)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="customerEmail" minOccurs="0" type="CommunicationContactTypeU">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Customer e-mail</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="retryProcess" minOccurs="0" type="StatusType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Cars retry process</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ruleReference" minOccurs="0" type="TicketingPriceSchemeType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Reference of the rule to be used for mark-up computation.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="action" minOccurs="0" type="ActionIdentificationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Used to indicate that mark-up computation must be bypassed.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ActionIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide details of action required or taken, the flights to which this action refers, and reasons for action taken.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="actionRequestCode" type="AlphaNumericString_Length1To3" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalBusinessSourceInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify ownership and creator of a PNR</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="originatorDetails" type="OriginatorIdentificationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">ORIGINATOR DETAILS</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressDetailsTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the details of an address.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="format" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address format</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="line1" type="AlphaNumericString_Length1To60">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address Field in free flow text</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify an address</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="addressUsageDetails" type="AddressUsageTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address Type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="addressDetails" minOccurs="0" type="AddressDetailsTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Format 1 - Home Delivery/Collection</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="city" minOccurs="0" type="AlphaNumericString_Length1To30">
        <xs:annotation>
          <xs:documentation xml:lang="en">City name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zipCode" minOccurs="0" type="AlphaNumericString_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">Postal Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="countryCode" minOccurs="0" type="AlphaNumericString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Country code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="regionDetails" minOccurs="0" type="CountrySubEntityDetailsTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey a sub-entity within a country : region, states..</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="locationDetails" minOccurs="0" type="LocationIdentificationTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Format 2 - Site Delivery/Collection</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressUsageTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To describe the usage of an address</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="purpose" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Address Type: - DEL for Delivery - COL for Collection</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AgreementIdentificationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the agreement details.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="agreementDetails" minOccurs="0" type="AgreementTypeIdentificationTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Agreement identification</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AgreementTypeIdentificationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">Identification of specific agreement type by code or name.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="code" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">- TK for Tracking option</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">Agreement description</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AssociatedChargesInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Identification of the additional charges that are associated to a product or service.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Set to VV to identify voucher value</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="comment" minOccurs="0" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">voucher text transmited to the car company</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BillableInformationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide billable information for a claim or encounter.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="billingInfo" maxOccurs="3" type="DiagnosisTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">billing number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommunicationContactDetailsTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">Communication number of a department or employee in a specified channel.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="email" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">Email</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="contactQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Contact qualifier. EM for Electronic mail</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommunicationContactTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">email</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="contact" type="CommunicationContactDetailsTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Customer contact</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">This segment is used to convey the non-air company information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="travelSector" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to qualify the company code, to identify the industry business it belongs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyCode" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the company code of a non-air company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyName" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the company name of a non-air company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accessLevel" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the type of access the non-air company has with Amadeus.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConsumerReferenceIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Identification of consumer references by type, company, number and reference to a traveller.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="referenceQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicator - 1  for ID (customer number) - CD for CD (customer discount number) - PC for PC (promotional code)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="referenceNumber" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Either the ID number, the CD number or the PC number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConsumerReferenceInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify consumer reference information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="customerReferences" type="ConsumerReferenceIdentificationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Consumer reference information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherCustomerRef" minOccurs="0" maxOccurs="2" type="ConsumerReferenceIdentificationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Additional consumer reference information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CountrySubEntityDetailsTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify a part of a country (eg county or part of              a city).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="code" type="AlphaNumericString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">State Code. Mandatory if CountryCode is US, CA, AU.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CreditCardAttributeDataType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Conveys attribute data of the credit card</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="securityId" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Conveys the security ID of the Credit Card.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DeliveryAndCollectionType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Delivery and Collection address</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="addressDeliveryCollection" type="AddressTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">This Segment is used to Delivery and Collection information:  Format 1- (Home Collection): - Address - City - State - Country - Zip Code  Format 2- (Site Collection): - Site Ref Id - Site Name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="phoneNumber" type="PhoneAndEmailAddressType">
        <xs:annotation>
          <xs:documentation xml:lang="en">This segment is used to carry phone number associated to a Delivery / Collection address</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DiagnosisTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the diagnosis and its type</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="billingDetails" type="AlphaNumericString_Length1To26">
        <xs:annotation>
          <xs:documentation xml:lang="en">Billing number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="billingQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Billing number indicator  - 901 for a Billing number - 902 fot a Billing reference number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FareCategoryCodesTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To designate non-system specific combinations of fare types.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="fareType" type="AlphaNumericString_Length1To6">
        <xs:annotation>
          <xs:documentation xml:lang="en">Rate Code (code set list not used)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FareQualifierDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the details which qualify a fare</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="fareCategories" type="FareCategoryCodesTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Rate Code Information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FlightDetailsQueryType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify outbound and inbound flight information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="inboundCarrierDetails" type="InboundCarrierDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">To provide information on the incarriage flight carrier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="inboundFlightDetails" type="InboundFlightNumberDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Reference of the incarriage flight</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="inboundArrivalDate" minOccurs="0" type="NumericInteger_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">arrival time</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FormOfPaymentDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the form of payment type, amounts, approval codes, and other related information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">Fop type (Cash, Credit card...)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="vendorCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Credit card vendor code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="creditCardNumber" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Credit card number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="expiryDate" minOccurs="0" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">expiry date (MMYY)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="extendedPayment" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">FOP purpose</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fopFreeText" minOccurs="0" type="AlphaNumericString_Length1To70">
