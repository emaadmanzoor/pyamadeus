                        <xs:sequence>
                          <xs:element name="feeSubType" type="SpecificDataInformationTypeI_67068S" />
                          <xs:element name="feeMonetaryInfo" type="MonetaryInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fee monetary info</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="feeFormOfPayment" minOccurs="0" type="FormOfPaymentTypeI">
