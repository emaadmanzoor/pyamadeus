                          <xs:element name="feeTaxes" minOccurs="0" type="TaxTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Taxes applicable on a fee</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
