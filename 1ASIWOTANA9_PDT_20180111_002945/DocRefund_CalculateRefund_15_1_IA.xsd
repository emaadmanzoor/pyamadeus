                            </xs:annotation>
                          </xs:element>
                          <xs:element name="couponPricingSubsequent" minOccurs="0" type="PricingTicketingSubsequentTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Coupon pricing information - fare basis and additional info</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="couponDateTimeInfo" minOccurs="0" type="DateAndTimeInformationTypeI">
