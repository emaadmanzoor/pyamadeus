                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="feeTypeInfo" type="SelectionDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Fee Type</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="feeSubGroup" maxOccurs="99">
