<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/TRFUUR_14_1_1A" xmlns="http://xml.amadeus.com/TRFUUR_14_1_1A" elementFormDefault="qualified">
  <xs:element name="DocRefund_UpdateRefundReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Refund Update response
Refund update message
Clone of TRFUUR 12.2.1  Update done for tracking purpose. Update of Data Element properties of composite 5004  and 5482 of CMN segment. XML type updated from Integer to Decimal.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="applicationErrorGroup" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Application Error Group Can contain up to five error messages</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="applicationErrorValue" type="ApplicationErrorValueType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This segment is used when an error or warning has been detected and must be displayed to the agent. If the error is directly associated to a specific field of the refund, the reference number of the ERV segment will contain its reference ID. If message to display is general to the refund (for example in case of warning), only the free text will be sent in the ERV message.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="errorText" minOccurs="0" type="FreeTextInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">error text</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="refundableDocument" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="userIdentification" type="UserIdentificationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">User identification mainly used for the agent code and the agent sign.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ruleId" type="ReferenceInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Convey the rule Id, client must used to display the refund record</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="reportingTransactionDetails" type="TransactionInformationForTicketingType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">reporting Transaction Details</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ticketNumber" minOccurs="0" maxOccurs="99" type="TicketNumberTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">*Used to reference refund document number (only for re-display use case)  *Used to reference other document impacted by the refund transaction (ATC refund performed on a ticket exchanged with ATU)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="dateTimeInformation" maxOccurs="2" type="StructuredDateTimeInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Current date (710) Possibly date of refund of the document (DR)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="referenceInformation" minOccurs="0" type="ReferenceInfoType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">References</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="travellerInformation" minOccurs="0" type="TravellerInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Information on the passenger to refund</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="rapidrewardaccount" minOccurs="0" type="FrequentTravellerIdentificationCodeType" />
              <xs:element name="ticket" minOccurs="0" maxOccurs="7">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Ticket Details. Up to 7 conjunctives</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ticketInformation" type="TicketNumberTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Ticket to refund information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ticketGroup" minOccurs="0" maxOccurs="4">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Group for Ticket Information</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="couponInformationDetails" type="CouponInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Coupons information details. Return only coupon avalaible for refund Up to 4 coupons</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="boardingPriority" minOccurs="0" type="PriorityTypeU">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Use to update boarding priority for Staff.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="actionIdentification" minOccurs="0" type="ActionIdentificationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for Staff update flag.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="referenceInformation" minOccurs="0" type="ReferenceInfoType_57269S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for Staff Validation Certificate.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="travellerPriorityInfo" minOccurs="0" type="TravellerPriorityDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Use for Staff: Staff number, Date of Joining and Cost Center.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="monetaryInformation" minOccurs="0" type="MonetaryInformationType" />
              <xs:element name="pricingDetails" minOccurs="0" type="PricingTicketingDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Pricing details</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="commission" minOccurs="0" type="CommissionInformationType" />
              <xs:element name="eventDetails" minOccurs="0" type="DeparturePlanEventType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To keep history of modifications on Fare used, paid, the tax refundable or canx fee</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="taxDetailsInformation" minOccurs="0" maxOccurs="198" type="TaxType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">List of refund taxes (up to 197 taxes may be present+one paid tax aggregate amount).  Previously paid aggregate sum of taxes must be convey using the 701 tax category.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="tourInformation" minOccurs="0" type="TourInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Tour information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="interactiveFreeText" minOccurs="0" maxOccurs="3" type="InteractiveFreeTextType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys different information carried as free-text: - Remarks - ...</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="fopGroup" minOccurs="0" maxOccurs="99" type="FOPRepresentationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Fop group (up to 99 fops)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="firstAndLastSegmentDates" minOccurs="0" type="StructuredPeriodInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the date of the first and the last segment of the original coupons</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="originAndDestination" minOccurs="0" type="OriginAndDestinationDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">The routing field identifies what routing the document was to be refunded. For manual refund routing information is not available and this will be up to the agent to enter routing in order to be able to calculate VAT. In cases where a manual document needs to be refunded the routing indicator is mandatory. (For full and partial refund this element can not be entered)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="refundedItinerary" minOccurs="0" maxOccurs="16" type="RefundedItineraryType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This default group is used to convey the refunded itinerary. The refunded itinerary is composed of the origin city/airport code, the flight operating airline code and the destination city/airport code. Origin and destination city/airport codes are contained in the ODI segment. Operating airline code is conveyed in the TRA segment. This default group can be repeated up to 16 times since a document can be composed of up to 16 coupons.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="refundedRoute" minOccurs="0" type="RoutingInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">The routing coresponding to the refunded itinerary.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="structuredAddress" minOccurs="0" type="StructuredAddressType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Used to convey any kind of structured address. For the time being, only billing address (AB) is supported.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="feeGroup" minOccurs="0">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Fees group</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="feeType" type="SelectionDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Fee type</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="feeSubGroup" maxOccurs="99">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">fee Sub Group</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="feeSubType" type="SpecificDataInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">fee sub type</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="feeMonetaryInfo" type="MonetaryInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">fee monetary Info</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="feesTaxes" minOccurs="0" maxOccurs="99" type="TaxTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fee taxes</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ActionIdentificationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide details of action required or taken, the flights to which this action refers, and reasons for action taken.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="actionRequestCode" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">provides type of required action, coded.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorValueType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the data in error and the erroneous value.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="dataLabel" minOccurs="0" type="AlphaNumericString_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">Application data identification of the data in which the error occurs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="occurenceNumber" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Occurrence number (1-n) of the data element where an error has been  detected.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dataValue" minOccurs="0" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">Data value in error or error message to display to the user.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CodedAttributeInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Convey coded key and corresponding value</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="attributeType" type="AlphaNumericString_Length1To5" />
      <xs:element name="attributeDescription" minOccurs="0" type="AlphaNumericString_Length1To256" />
      <xs:element name="language" minOccurs="0" type="AlphaNumericString_Length1To3" />
      <xs:element name="encoding" minOccurs="0" type="AlphaNumericString_Length1To8" />
      <xs:element name="source" minOccurs="0" type="AlphaNumericString_Length1To3" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CodedAttributeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used to have tag value without code list for tag</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="attributeFunction" minOccurs="0" type="AlphaNumericString_Length1To3" />
      <xs:element name="attributeDetails" maxOccurs="99" type="CodedAttributeInformationType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommissionDetailsType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the commission conditions, amount, currency, party paying and any related free text.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission type : 'NEW' --) New commission 'OLD' --) Old Commission 'XLP' --) Commission on cancellation Penalty 'FMA' --) Airline Commission A 'FMB' --) Airline Commission B</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amount" minOccurs="0" type="NumericDecimal_Length1To18">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="freeText" minOccurs="0" type="AlphaNumericString_Length1To70" />
      <xs:element name="rate" minOccurs="0" type="NumericDecimal_Length1To8">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission percentage</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommissionInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify information about a commission.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="commissionDetails" type="CommissionDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission details</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherComDetails" minOccurs="0" maxOccurs="8" type="CommissionDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Commission details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Code or name to identify a company and any associated companies.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="operatingCompany" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Used to convey an airline code. The airline code can be replaced by -- (in case of ARNK segment).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CouponInformationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the coupon number, status, value, and other related information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cpnNumber" type="AlphaNumericString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Coupon number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cpnStatus" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Coupon status</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="settlementAuthorization" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Settlement Authorisation Code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CouponInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify data specific to a value (flight) coupon.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="couponDetails" type="CouponInformationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Coupon information details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DataInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify specific data and a quantity related to the data.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="indicator" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">The carrier fee application code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DataTypeInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of data to be sent and to qualify the data when required.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">To specify the type of data, i.e. staff reservation entitlement, status, type of travel, or ticket type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="statusEvent" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Reservation entitlement/status, type of travel, ticket type</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DeparturePlanEventType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To keep a trace after updates</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="reference" type="EventReferenceType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Event and activity references.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EventReferenceType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide the Event identification</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="activityCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">activityCode</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="activityDependencyCode" minOccurs="0" maxOccurs="99" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">activityDependencyCode</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FOPRepresentationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">This group will be used to convey all fop informations</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="formOfPaymentInformation" type="FormOfPaymentTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Form of payment</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="interactiveFreeText" maxOccurs="2" type="InteractiveFreeTextType">
        <xs:annotation>
          <xs:documentation xml:lang="en">This interactive free text is used to pass the complete form of payment and the  customer file reference fields</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="formOfPaymentattribute" minOccurs="0" type="CodedAttributeType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FormOfPaymentDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the form of payment type, amounts, approval codes, and other related information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">the type of the fop</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="amount" minOccurs="0" type="NumericDecimal_Length1To18">
        <xs:annotation>
          <xs:documentation xml:lang="en">Form of payment amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sourceOfApproval" minOccurs="0" type="AlphaNumericString_Length1To3" />
      <xs:element name="authorisedAmount" minOccurs="0" type="NumericInteger_Length1To18" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FormOfPaymentTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey details describing the form of payment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="formOfPayment" type="FormOfPaymentDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Form of payment details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FreeTextDetailsType">
    <xs:sequence>
      <xs:element name="textSubjectQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">text Subject Qualifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="informationType" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Type of information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="source" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">source</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="encoding" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">encoding</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FreeTextInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">This segment is used to convey the free text</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="freeTextDetails" type="FreeTextDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Details of the error</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="freeText" type="AlphaNumericString_Length1To199">
        <xs:annotation>
          <xs:documentation xml:lang="en">Error text</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FreeTextQualificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the type, purpose, and language of free text and whether any action is required.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="textSubjectQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Text subject qualifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="informationType" minOccurs="0" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Text type information to identify the IFT segment data.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FrequentTravellerIdentificationCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify frequent traveler information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="frequentTravellerDetails" maxOccurs="99" type="FrequentTravellerIdentificationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Frequent Traveller Info</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FrequentTravellerIdentificationType">
    <xs:sequence>
