      <xs:documentation xml:lang="en">Update TST

</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="pricingInformation" minOccurs="0" type="PricingTicketingSubsequentTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">in order to update the sales indicator.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="actionInformation" type="ActionIdentificationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">TST update action information such as FUP (full update) or PUP (partial update).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="fareReference" type="ItemReferencesAndVersionsType">
          <xs:annotation>
            <xs:documentation xml:lang="en">TST reference number. Ordering information is not relevant here.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="fareDataInformation" minOccurs="0" type="MonetaryInformationTypeI_150994S">
          <xs:annotation>
            <xs:documentation xml:lang="en">Specify all fare data information (base fare, equivalent fare...)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="reasonCode" minOccurs="0" maxOccurs="99">
          <xs:annotation>
            <xs:documentation xml:lang="en">reason for agent override</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="reasonCodes" type="AttributeType_21097S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Updated field and corresponding reason for update and travel agent comment</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="reasonCodeComment" maxOccurs="2" type="FreeTextInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">reason for override: OTHER SODA  RULE 85  INTERNET DING  ADVERTISED FARE NOT LOADED  US TAX ADJUSTMENT  CUSTOMER CONVENIENCE  EXTRA SEAT  LOYALTY HONOR PROTECT FARE  TWO HOUR RULE NAME MISMATCH NAME CHANGE DECEASED</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="taxInformation" minOccurs="0" maxOccurs="120">
