            <xs:documentation xml:lang="en">Other fare information such as Fare Calculation,...</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="statusInformation" minOccurs="0" type="StatusTypeI">
          <xs:annotation>
